---
const {image, title, content, linkWeb, linkRepo } = Astro.props;
---
<div>
    <img src={image} alt="porjectos">
    <img src="astro.svg" alt="" id="card-image">
    <h3 class="text-2xl">{title}</h3>
    <p>{content}</p>
    <a href={linkWeb} class="text-blue-500">Ver proyecto</a>
    <a href={linkRepo} class="text-blue-500">Ver repositorio</a>
</div>
</div>